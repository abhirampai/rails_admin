<% if @created_record.valid? %>
    document.querySelector(".modal-check").checked = false
    document.querySelector(".js-content-modal").innerHTML = ""
    document.querySelector(".dashboard-table").insertAdjacentHTML("beforeend", "<%= j render partial: "rails_admin/dashboards/table/row", locals: { row: @created_record }%>")
    alert("Created new record")
<% else %>
    alert("Something went wrong")
<% end %>