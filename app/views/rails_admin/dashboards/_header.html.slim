ruby:
    class_name = @model.name
	js_controller = "dashboard--show"

.header data-controller=js_controller
    .table-name = class_name
    .search-box
        input.search-input[
            type="text"
            data-target="#{js_controller}.searchTerm"
            data-action="keydown.enter->#{js_controller}#searchTable"
            placeholder="Search #{@searchable_columns.join(", ")}"]